    <script>
        let slideIndexa = 0;
        showSlides();

        function showSlides() {
            let i;
            let slides = document.getElementsByClassName("mySlider");
            for (i = 0; i < slides.length; i++) {
                slides[i].style.display = "none";
            }
            slideIndexa++;
            if (slideIndexa > slides.length) {
                slideIndexa = 1
            }
            slides[slideIndexa - 1].style.display = "block";
            setTimeout(showSlides, 5000); // Change image every 2 seconds
        }
    </script>

    <script>
